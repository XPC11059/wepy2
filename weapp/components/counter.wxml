
<view class="counter {{style}}">
  <button size="mini" data-wpy-evt="11-0" bindtap="_proxy">  +  </button>
  <button size="mini" data-wpy-evt="11-1" bindtap="_proxy">  -  </button>
  <button size="mini" data-wpy-evt="11-2" bindtap="_proxy">  INCREMENT  </button>
  <button size="mini" data-wpy-evt="11-3" bindtap="_proxy">  DECREMENT  </button>
  <button size="mini" data-wpy-evt="11-4" bindtap="_proxy">  ASYNC INCREMENT  </button>
  <label class="count"> {{counter}} </label>
</view>
